{
  "name": "@ticatec/config-loader",
  "version": "0.0.4",
  "description": "A versatile configuration loader for Node.js, supporting YAML format and various sources like local files, Consul, and Nacos, with include and deep merge capabilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README*.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "npm run build",
    "lint": "echo \"No linting configured\"",
    "typecheck": "tsc --noEmit",
    "publish-public": "npm publish --access public"
  },
  "keywords": [
    "config",
    "configuration",
    "loader",
    "yaml",
    "consul",
    "nacos",
    "node.js",
    "typescript",
    "microservices",
    "devops"
  ],
  "author": {
    "name": "Henry Feng",
    "email": "huili.f@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ticatec/config-loader.git"
  },
  "bugs": {
    "url": "https://github.com/ticatec/config-loader/issues"
  },
  "homepage": "https://github.com/ticatec/config-loader#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "consul": "^2.0.1",
    "dotenv": "^16.4.7",
    "log4js": "^6.7.0",
    "nacos": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "consul": "^2.0.0",
    "nacos": "^2.6.0"
  },
  "peerDependenciesMeta": {
    "consul": {
      "optional": true
    },
    "nacos": {
      "optional": true
    }
  }
}